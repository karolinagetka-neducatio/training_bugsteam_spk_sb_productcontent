<?xml version="1.0" encoding="UTF-8"?>
<manifest xmlns="http://www.imsproject.org/xsd/imscp_rootv1p1p2" xmlns:adlcp="http://www.adlnet.org/xsd/adlcp_rootv1p2"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          identifier="com.springernature.skillful-dsb-sb-ls-level-1.scorm12" version="1"
          xsi:schemaLocation="http://www.imsproject.org/xsd/imscp_rootv1p1p2 imscp_rootv1p1p2.xsd http://www.imsglobal.org/xsd/imsmd_rootv1p2p1 imsmd_rootv1p2p1.xsd http://www.adlnet.org/xsd/adlcp_rootv1p2 adlcp_rootv1p2.xsd"
          xmlns:mm="http://www.macmillan.com/macmillan-scorm-extension"
          >
    <metadata>
        <schema>ADL SCORM</schema>
        <schemaversion>1.2</schemaversion>
    </metadata>
    {{#each organisations}}
        <organizations default="{{id}}">
            <organization identifier="{{id}}">
                <title>{{title}}</title>
                {{> items }}
            </organization>
        </organizations>
    {{/each}}
    <resources>
        <resource identifier="activity_player" type="webcontent" adlcp:scormtype="sco" href="index.html">
            <file href="index.html"/>
        </resource>
    </resources>
</manifest>